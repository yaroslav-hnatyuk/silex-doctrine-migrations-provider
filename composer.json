{
    "name": "kurl/silex-doctrine-migrations-provider",
    "description": "A doctrine migrations provider for silex",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.1",
        "pimple/pimple": "^3.5",
        "silex/api": "^2.0",
        "doctrine/migrations": "^3.0",
        "symfony/console": "^5.3",
        "symfony/http-foundation": "^5.3"
    },
    "require-dev": {
        "doctrine/orm": "^2.9",
        "silex/silex": "^2.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Kurl\\Silex\\Provider\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Kurl\\Silex\\Provider\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
